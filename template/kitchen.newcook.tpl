<li class="clearfix" onclick="showprogressdetail('[blksql.cKdPesanan;noerr;block=li]');">
    <div class="pull-left">
        <p>
            <h5><strong>#[blksql.IdPesanan;noerr;block=li]</strong> [blksql.vNmTamu;noerr;block=li]</h5>
        </p>
        <p><i class="fa fa-clock-o"></i> <abbr class="timeago" title="[blksql.dTglKirimKitchen;noerr;block=li]" >[blksql.dTglKirimKitchen;noerr;block=li]</abbr></p>
                                                                        
    </div>
    <div class="text-right pull-right">
        <h4 class="cost">[blksql.vNmTable;noerr;block=li]</h4>
        <p>
            <span class="label label-warning arrow-in-right"><i class="fa fa-cog"></i> In Progress</span>
        </p>
    </div>
    <div class="clearfix"></div>
    <div class="progress progress-sm">
      <div class="progress-bar progress-bar-warning" id="onprogress-persen" role="progressbar" aria-valuenow="85" aria-valuemin="0" aria-valuemax="100">
        <span class="sr-only" id="onprogress-persen-info"></span>
      </div>
    </div>
</li>
